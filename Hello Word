import java.util.Scanner;
public class Main{
  public static int N;
  public static void main(String[] args){
  Scanner in = new Scanner(System.in);
  System.out.print("Ingin tampilkan berapa kali :  ");
  N = in.next();
    for(int i = 0; i<N; i++){
      System.out.println("Hello World");
    }
    System.out.println("------------------");
  }
}
